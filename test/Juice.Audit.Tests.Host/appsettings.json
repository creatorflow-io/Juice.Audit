{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Audit": {
    "Filters": [
      {
        "Methods": [ "POST", "PUT", "DELETE" ]
      },
      {
        "IsExcluded": true,
        "Path": "/#/negotiate"
      },
      {
        "IsExcluded": true,
        "Path": "/error/*"
      },
      {
        "StatusCodes": [ 400, 500 ]
      }
    ]
  },
  "AllowedHosts": "*"
}
